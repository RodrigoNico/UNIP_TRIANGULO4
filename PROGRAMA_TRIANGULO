def triangulo(a, b, c):
    if a + b >= c and b + c >= a and c + a >= b:
        return True
    else:
        return False

def tipoTriangulo(a, b, c):
    if triangulo(x, y, z) == False:
        print("Seu Triangulo é impossível.\n")
    else:
        if a == b and b == c:
            print("Seu Triangulo é Equilátero")
        elif a == b or b == c or a == c:
            print("Seu triangulo é Isósceles")
        else:
            print("Seu triangulo é escaleno")
